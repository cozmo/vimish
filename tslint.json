{
  "extends": "tslint:latest",
  "rules": {
    "interface-name": [false],
    "object-literal-sort-keys": [false],
    "max-line-length": [true, 160],
    "curly": false,
    "ordered-imports": false,
    "one-line": [false],
    "member-ordering": [true,
      "variables-before-functions"
    ]
  }
}